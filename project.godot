; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="doom-godot"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://images/icon.png"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=640
window/size/height=400
window/size/always_on_top=true
window/size/test_width=960
window/size/test_height=600
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[input]

ui_show_hide_hud=[ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777218,"unicode":0,"echo":false,"script":null)
 ]

[layer_names]

2d_physics/layer_1="Walls"
2d_physics/layer_2="Player"

[rendering]

quality/filters/anisotropic_filter_level=1
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
