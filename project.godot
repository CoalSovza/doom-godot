;   param=value ; assign values to parameters

config_version=3

[application]

config/name="doom-godot"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://images/icon.png"

[display]

window/size/width=320
window/size/height=200
window/size/always_on_top=true
window/size/test_width=640
window/size/test_height=400
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[input]

ui_show_hide_hud=[ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777218,"unicode":0,"echo":false,"script":null)
 ]

[layer_names]

2d_physics/layer_1="Walls"
2d_physics/layer_2="Player"

[rendering]

environment/default_environment="res://default_env.tres"
